{
  "name": "javabattery",
  "description": "Highlight your battery percentage!",
  "author": {
    "name": "OyewoleOyedeji",
    "email": "urologistlividly906@simplelogin.com"
  },
  "version": "1.0.1",
  "private": true,
  "main": "out/main.js",
  "productName": "Javabattery",
  "scripts": {
    "build": "nuxt generate",
    "dev": "nuxt dev",
    "debug": "vue-devtools",
    "icons": "magick convert -background transparent build/icon.svg -define icon:auto-resize build/icons/icon.ico && magick convert -background transparent build/icon.svg build/icons/icon.png && cp build/icons/icon.ico public/favicon.ico",
    "package": "electron-builder --dir",
    "postinstall": "npm run icons && nuxt prepare && node node_modules/vite-plugin-electron-renderer/install.js",
    "release": "nuxt generate && electron-builder"
  },
  "devDependencies": {
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.6.5",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "bootstrap-icons": "^1.10.3",
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0",
    "nuxt": "3.3.2",
    "nuxt-electron": "^0.4.4",
    "nuxt-fonty": "^1.0.0",
    "vite-electron-plugin": "^0.8.2",
    "vite-electron-renderer": "^0.0.3",
    "vite-plugin-electron-renderer": "^0.13.13",
    "vue-devtools": "^5.1.4"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.12",
    "battery": "^1.0.1",
    "electron-updater": "^5.3.0",
    "nuxt-battery": "^1.1.10",
    "tinydate": "^1.3.0"
  },
  "optionalDependencies": {
    "@types/node": "^18.15.11"
  }
}
